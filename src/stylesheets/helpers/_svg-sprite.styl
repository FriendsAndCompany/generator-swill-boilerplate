$svg-common 
	background-repeat no-repeat
	background-image url(../images/svg-sprite.svg)

.svg-ic
	background-position 0 0
	@extend $svg-common

.svg-ic-dims
	width 100px
	height 99.997px

.svg-icon
	background-position 100% 0
	@extend $svg-common

.svg-icon-dims
	width 100px
	height 99.997px

.svg-icon-hover
	background-position 0 99.9940001799946%
	@extend $svg-common

.svg-icon-hover-dims
	width 100px
	height 99.997px

